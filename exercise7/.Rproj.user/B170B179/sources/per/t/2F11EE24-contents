oddnumber1<- function(x){
  x1 <- read.csv(x)
  counter <- 0
  for (row in x1) {
    counter<- counter + 1
    print(counter)
  }
}

oddnumber <- function(x){
  x1 <- read.csv(x)
  counter <- 0
  for (rows in x1) {
    counter <- counter + 1
    print(rows)
  }
      
      
    


  

    

  
    